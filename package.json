{
  "name": "gulp-remove-code",
  "version": "2.0.0",
  "description": "Remove sections of code from files based on conditions",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && mocha",
    "lint": "eslint --max-warnings 0 -c package.json index.js test/main.js"
  },
  "files": [
    "index.js"
  ],
  "engines": {
    "node": ">=4.8.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crissdev/gulp-remove-code.git"
  },
  "keywords": [
    "gulpplugin",
    "gulp-plugin",
    "gulp",
    "remove",
    "remove code",
    "code",
    "strip",
    "strip code",
    "stream"
  ],
  "author": "crissdev <criss.trifan@gmail.com> (https://crissdev.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crissdev/gulp-remove-code/issues"
  },
  "homepage": "https://github.com/crissdev/gulp-remove-code",
  "dependencies": {
    "bufferstreams": "1.1.3",
    "escape-string-regexp": "^1.0.5",
    "event-stream": "^3.3.4",
    "fancy-log": "^1.3.2",
    "plugin-error": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^4.18.2",
    "eslint-config-gulp": "^2.1.0",
    "mocha": "^5.0.3",
    "vinyl": "^2.1.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "max-len": [
        1,
        120
      ]
    }
  }
}
